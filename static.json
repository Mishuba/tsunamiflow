{
  "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*",
      "Cache-Control": "public, max-age=86400"
    }
  },
  "rewrites": [
    { "source": "/Server/*", "destination": "https://world.tsunamiflow.club/:splat" },
    { "source": "/Music/*", "destination": "https://radio.tsunamiflow.club/Music/:splat"}
  ],
  "cleanUrls": true,
  "trailingSlash": false
}